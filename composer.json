{
    "require": {
        "laravel/laravel": "4.1.*",
        "cartalyst/sentry": "2.1.*",
        "edvinaskrucas/notification": "1.*",
        "intervention/image": "2.0.17",
        "intervention/imagecache": "dev-master",
        "bllim/datatables": "*",
        "robclancy/presenter": "1.1.*",
        "artdarek/oauth-4-laravel": "dev-master",
        "toin0u/geocoder-laravel": "@stable",
        "doctrine/dbal": "@stable"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.cartalyst.com"
        }
    ],
    "require-dev": {
        "way/generators": "2.*",
        "fzaninotto/faker": "v1.3.0"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
            "app/services",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	}
}